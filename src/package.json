{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "stylelint": "npx stylelint './resources/sass/**/*.scss'",
        "stylelint:fix": "npm run stylelint --fix",
        "prettier": "npm run prettier:styles && npm run prettier:js && npm run prettier:php",
        "prettier:styles": "npx prettier './resources/sass/**/*.{css,scss}' --write",
        "prettier:js": "npx prettier './resources/js/**/*.{js,vue}' --write",
        "prettier:php": "npx prettier './app/**/*.php' './tests/**/*.php' --write"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.1.7",
        "@inertiajs/inertia-vue": "^0.1.2",
        "axios": "^0.19",
        "bootstrap-vue": "^2.3.0",
        "clipboard": "^2.0.6",
        "collect.js": "^4.22.0",
        "cross-env": "^7.0",
        "js-base64": "^2.5.2",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "noty": "^3.2.0-beta",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "string": "^3.3.3",
        "tabler": "^1.0.0-alpha.6",
        "tabler-icons": "^1.4.0",
        "vue": "^2.6.11",
        "vue-apexcharts": "^1.5.3",
        "vue-json-pretty": "^1.6.3",
        "vue-meta": "^2.3.1",
        "vue-template-compiler": "^2.6.11"
    },
    "devDependencies": {
        "@prettier/plugin-php": "^0.14.2",
        "prettier": "^2.0.5",
        "stylelint": "^13.5.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-scss": "^3.17.2"
    }
}
